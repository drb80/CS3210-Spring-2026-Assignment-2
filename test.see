n = 10;
factorial = 1;
i = 1;

while (i <= n) {
    factorial = factorial + i;
    i = i + 1;
}

print(factorial);

power = 1;
while (power + power < n) {
    power = power + power;
}
print(power);

counter = n;
while (counter > 0) {
    print(counter);
    counter = counter - 1;
}

x = 15;
y = 20;

if (x < y) {
    print(1);
} else {
    print(0);
}
